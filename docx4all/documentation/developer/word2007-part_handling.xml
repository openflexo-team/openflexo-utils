                <w:body>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Title"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">Word 2007 - part handling idiosyncracies</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">If you use docx4all to create a docx, its </w:t>
                            <w:t xml:space="preserve">document.xml.rels will contain just one part: styles.xml</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">As soon as you open the docx in Word 2007, Word will add the following parts:</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">- webSettings.xml</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">- settings.xml</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">- theme/theme1.xml</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">- fontTable.xml</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">Then, when you add a heading, it will also add the following parts:</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">- header1.xml (as expected)</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">- glossary/document.xml etc</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">- endnotes.xml (!)</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">- footnotes.xml (!)</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">Word orders parts consistently in the following order:</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
                        <w:r>
                            <w:t xml:space="preserve">At the beginning:</w:t>
                        </w:r>
                    </w:p>
                    <w:p>
                        <w:pPr>
                            <w:pStyle w:val="Normal"/>
                        </w:pPr>
